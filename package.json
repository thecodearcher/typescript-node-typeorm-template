{
  "name": "typescript-node-typeorm-template",
  "version": "1.0.0",
  "description": "Node with express using typescript",
  "main": "./dist/index.js",
  "scripts": {
    "post-install": "mkdir ./logs",
    "compile": "tsc",
    "start": "tsc && node ./dist",
    "watch": "nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "author": "Brian Iyoha",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@types/hapi__joi": "^16.0.12",
    "app-root-path": "^2.2.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.22.0",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "mysql2": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^1.11.1",
    "typeorm": "^0.2.24",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.6",
    "@types/express-rate-limit": "^3.3.4",
    "@types/helmet": "0.0.45",
    "@types/lodash": "^4.14.150",
    "@types/morgan": "^1.9.0",
    "@types/node": "^10.17.21",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.8.3"
  }
}
